		<!-- #################################################### Work Update #################################################### -->
		<record model="ir.ui.view" id="bpl_work_update_form">
			<field name="name">bpl.work.update.form</field>
			<field name="model">bpl.work.update</field>
			<field name="arch" type="xml">

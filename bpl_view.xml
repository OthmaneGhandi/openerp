		<!-- #################################################### START DEDUCTION 
			MASTER DATA #################################################### -->

		<record model="ir.ui.view" id="bpl_deduction_master_data">
			<field name="name">bpl.deduction.master.data.form</field>
			<field name="model">bpl.deduction.master.data</field>
			<field name="arch" type="xml">
				<form string='bpl_deduction_master_data' version='7.0'>
					<sheet>
						<group>
							<group>
								<field name='based_on' />
								<field name='bpl_company_id' />
								<field name='bpl_estate_id' attrs="{'invisible':[('based_on','=','company')]}" />
								<field name='bpl_division_id'
									attrs="{'invisible':['|',('based_on','=','company'),('based_on','=','estate')]}" />
							</group>
						</group>
						<div name="Deduction Master Data"></div>
						<separator string='Deduction Master Data' />
						<notebook>
							<page string="Fixed Deductions">
								<field name='fixed_deduction_ids' nolabel='1'>
									<tree string='List' editable='bottom'>
										<field name='deduction_id' invisible="1" />
										<field name='deduction_name' />
										<field name='deduction_type' invisible="1" />
										<field name='rate' invisible="1" />
										<field name='amount' invisible="1" />
										<field name='active' invisible="1" />
									</tree>
								</field>
								<br />
								<br />
							</page>
							<page string="Variable Deductions">
								<field name='variable_deduction_ids' nolabel='1'>
									<tree string='List' editable='bottom'>
										<field name='deduction_id' invisible="1" />
										<field name='deduction_name' />
										<field name='active' invisible="1" />
									</tree>
								</field>
								<br />
								<br />
							</page>
							<page string="Special Deductions">
								<field name='special_deduction_ids' nolabel='1'>
									<tree string='List' editable='bottom'>
										<field name='deduction_id' invisible="1" />
										<field name='special_deduction_type' />
										<field name='bank_id'
											attrs="{'invisible': [('special_deduction_type','in',('loan','union','insurance'))]}" />
										<field name='union_id'
